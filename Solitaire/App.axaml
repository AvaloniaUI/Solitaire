<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Solitaire"
             Name="Solitaire"
             x:Class="Solitaire.App">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <MergeResourceInclude Source="Styles/Common.axaml" />
                <MergeResourceInclude Source="Styles/Window.axaml" />
                <MergeResourceInclude Source="Styles/Button.axaml" />
                <MergeResourceInclude Source="Styles/PlayingCard.axaml" />
                <MergeResourceInclude Source="Styles/CardsStyle.axaml" />
                <MergeResourceInclude Source="Styles/SuitMarker.axaml" />
                <MergeResourceInclude Source="Styles/TrayContainer.axaml" />
                <MergeResourceInclude Source="Styles/CardStackPlacementControl.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>